    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.0",
    "clsx": "1.2.1",
    "contentlayer": "^0.3.2",
    "date-fns": "2.30.0",
    "framer-motion": "^10.12.9",
    "gray-matter": "4.0.3",
    "gsap": "^3.11.5",
    "lodash.debounce": "4.0.8",
    "lottie-web": "5.9.6",
    "next": "^13.4.1",
    "next-contentlayer": "^0.3.2",
    "next-mdx-remote": "4.1.0",
    "react": "18.2.0",
    "react-device-detect": "2.2.2",
