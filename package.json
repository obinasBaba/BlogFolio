    "rehype-code-titles": "1.2.0",
    "rehype-img-size": "1.0.1",
    "rehype-pretty-code": "^0.9.2",
    "rehype-prism-plus": "1.5.0",
    "rehype-slug": "5.0.1",
    "remark-gfm": "3.0.1",
